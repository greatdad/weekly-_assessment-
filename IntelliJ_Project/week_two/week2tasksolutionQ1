fun main(){    //ten if statements    if ((6 > 4) || (4 > 6)) {        println("Android Class B rocks")        println("The facitators are awesome")    }    var sco : Boolean sco = ((4==4) &&(-1==-1))        var ress : Boolean = sco    if(sco){        println(ress)    }    if (50 > 49) {        println("50 is greater than 49")    }    var testNumb: Int = 1    if (testNumb >= 0) {        println("$testNumb is a positive value")    }    if (testNumb <= 0) {        println("$testNumb is a negative value")    }    var testNumb1: Int    testNumb1 = 3    if (testNumb1 > testNumb) {        println("$testNumb1 is greater than $testNumb")    }    if (testNumb1 != testNumb) {        println("$testNumb1 is not equal to $testNumb")    }    if (testNumb1 == testNumb) {        println("$testNumb1 is equal to $testNumb")    }    var checkPass = "Righteousness"    var passL = checkPass.length    if (checkPass.length >= 9){        println("You pass code entered is accepted, code length is $passL")    }    if(4 ==4){        println("Programming is fun")    }    println("5 conditional expression using when: ")    getCond1()    getCond2()    getCond3()    getCond4()    getCond5()}// 5 conditional expressings using whenfun getCond1() {    var rangee = 6    when(rangee){        in 1..5 ->println("Input is provided from the range 1 to 5")        in 6..10 ->println("Input is provided from the range 6 to 10")        else->println("Not within the specified ranges")    }}fun getCond2() {    var classAttendance : String = "Gift"    var classList : String = when(classAttendance) {        "Edoh" -> "Edoh is present Sir"        "Elohor" -> "Elohor is present Sir"        "Dorabella" -> "Dorabella is in class"        "Gift" -> "Gift is honourably present sir"        "Henry" -> "Henry is present"        else -> "Reported sick"    }    println(classList)}fun getCond3() {    var numb : Int = 5    var sayNumb = when(numb) {        1 -> "One"        2 -> "Two"        3 -> "Three"        4 -> "Four"        5 -> "Five"        6 -> "Six"        7 -> "Seven"        8 -> "Eight"        9 -> "Nine"        else -> "Invalid number"    }    println("You provided $numb")}fun getCond4() {    var entryData : Int = 3    var result = when(entryData) {        1 -> println("One")        2 -> println("Two")        3 -> println("Three")        4 -> println("Four")        5 -> println("Five")        6 -> println("Six")        7 -> println("Seven")        8 -> println("Eight")        9 -> println("Nine")        else ->println("Invalid entry number") }}fun getCond5() {    var rain : Int = 7    when(rain){        1,2,3,4,5 ->            println("it is dry season")        6,7,8,9,10 ->            println("" +                    "it is raining")        else ->println("Invalid Nigeria season")    }}  
